{
  "name": "boilerplate-react-baobab-webpack",
  "version": "0.0.3",
  "description": "Yet another boilerplate for react, es6, baobab, webpack 2",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:server watch:build",
    "start:client": "npm-run-all --parallel watch:server webpack_dev",
    "clean": "rm -r public/* &&  npm run webpack",
    "webpack_dev": "node_modules/.bin/webpack-dev-server",
    "webpack": "webpack",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon server.js --watch server.js",
    "build": "webpack --config webpack.config.production.js",
    "lint": "./node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wirednkod/boilerplate-react-baobab-webpack.git"
  },
  "keywords": [
    "react",
    "es6",
    "baobab",
    "webpack"
  ],
  "engines": {
    "node": "6.9.5"
  },
  "author": "Nikolaos Kontakis (wirednkod)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wirednkod/boilerplate-react-baobab-webpack/issues"
  },
  "homepage": "https://github.com/wirednkod/boilerplate-react-baobab-webpack#readme",
